{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "standard", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"camelcase": "off",
		"no-useless-constructor": "off",
		"no-unused-expressions": "off"
	}
}
